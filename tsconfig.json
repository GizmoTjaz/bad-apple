{
    "compilerOptions": {
        "module": "CommonJS",
        "outDir": "./build",
        "rootDir": "./src",
        "target": "ES2015",
        "strict": true,
        "noImplicitAny": false,
        "esModuleInterop": true,
		"baseUrl": "src",
		"paths": {
			"@/*": ["/*"],
			"@utils/*": ["utils/*"],
			"@typings/*": ["typings/*"],
			"@components/*": ["components/*"]
		}
    }
}